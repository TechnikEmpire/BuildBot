  {
    "version": "1.0.0-*",
    "buildOptions": {
      "emitEntryPoint": true,
      "preserveCompilationContext": true,
      "debugType": "portable",
      "platform": "anycpu",
      "outputName": "BuildBot"
    },
    "dependencies": {
      "BuildBotCore": "1.1.0",
      "Microsoft.CodeAnalysis.Common": "1.3.2",
      "Microsoft.CodeAnalysis.CSharp": "1.3.2",
      "Microsoft.NETCore.Runtime.CoreCLR": "1.0.4",
      "Microsoft.NETCore.Runtime": "1.0.1",
      "Microsoft.NETCore.Portable.Compatibility": "1.0.2",
      "System.Runtime": "4.1.0",
      "System.Runtime.Loader": "4.0.0",
      "NETStandard.Library": "1.6.0",
      "SharpCompress": "0.12.4",
      "System.Diagnostics.Process": "4.1.0",
      "System.Net.Http": "4.1.0",
      "System.Net.Requests": "4.0.11",
      "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
      "System.Text.RegularExpressions": "4.1.0",
      "CommandLineParser.Unofficial.CoreBeta": "2.1.0-beta",
      "System.Security.Cryptography.Hashing.Algorithms.Analyzers": "1.1.0",
      "System.Security.Cryptography.Algorithms": "4.2.0",
      "System.Runtime.InteropServices": "4.1.0"      
    },
    "frameworks": {
      "netcoreapp1.0": {
        "dependencies": {
          "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0"
          },
          "System.Runtime": "4.1.0"
        },
        "imports": [
          "netcoreapp1.0"
        ]
      }
    },
    "runtimes": {
      "win10-x64": {},
      "win81-x64": {},
      "win8-x64": {},
      "win7-x64": {}
    },
    "configurations": {
      "Debug": {
        "buildOptions": {
          "define": [
            "DEBUG",
            "TRACE"
          ],
          "optimize": false,
          "preserveCompilationContext": true,
          "debugType": "portable",
          "compile": [
            "Abstractions",
            "Extensions",
            "Services"
          ]
        }
      },
      "Release": {
        "buildOptions": {
          "define": [
            "RELEASE",
            "TRACE"
          ],
          "optimize": true,
          "preserveCompilationContext": true,
          "debugType": "portable",
          "compile": [
            "Abstractions",
            "Extensions",
            "Services"
          ]
        }
      }
    }
  }